<div class="table-responsive">
  <table class="table table-striped" id="requests">
    <thead>
    <tr>
      <th>Id</th>
      <th>customer_id</th>
      <th>initiated_on</th>
      <th>service_selected</th>
      <th>customer_notes</th>
      <th>address</th>
      <th>booking_date</th>
      <th>booking_slot</th>
      <th>status</th>
      <th>coupon_code</th>
      <th>amount</th>
      <th>rating</th>
      <th>customer_name</th>
      <th>phone</th>
      <th>email</th>
      <th>address</th>
      <th>source of request</th>
    </tr>
    </thead>
    <tbody>
    <% @orders.each do |order| %>
        <tr>
          <td><%= order["id"] %></td>
          <td><%= order["customer_id"] %></td>
          <td><%= order["initiated_on"] %></td>
          <td><%= order["service"] %></td>
          <td><%= order["customer_comments"] %></td>
          <td><%= order["address"] %></td>
          <td><%= order["booking_date"] %></td>
          <td><%= order["booking_slot"] %></td>
          <td><%= order["status"] %></td>
          <td><%= order["coupon_code"] %></td>
          <td><%= order["amount"] %></td>
          <td><%= order["rating"] %></td>
          <td><%= order["customer_name"] %></td>
          <td><%= order["phone"] %></td>
          <td><%= order["email"] %></td>
          <td><%= order["address"] %></td>
          <td><%= order["origin_of_request"] %></td>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>